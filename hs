<script type="text/javascript">
jQuery(function($) {
	var showHelp = function(evt, isSmallscreen) {
		if (isSmallscreen) {
			$('#signin-help').show();
			$('.help-button').hide();
		} else {
			$('#signin-help').hide();
			$('.help-button').show().on('click', function() {
				if ($('#signin-help').is(':visible')) {
					$(this).removeClass('active');
					$('#signin-help').fadeOut();
				} else {
					$(this).addClass('active');
					$('#signin-help').fadeIn();
				}
			});
		}
	};
	
	$(document.body).bind('smallscreen', showHelp);
	showHelp(null, $(document.body).hasClass('is-smallscreen'));

	$('#loginform input:visible:first').focus();

	$('#userName').on('keyup', function() {
		var index = $(this).val().toLowerCase().indexOf('@live.warw');
		if (index > 0) {
			var trimmed = $(this).val().substring(0, index).toLowerCase();

			if (!$('#inputHint').is(':visible')) {
				$('#inputHint').html('It looks like you\'re typing a Live@Edu username. You only need the first part (<strong>'+trimmed+'</strong>) to log in here.').fadeIn();
			}
		} else {
			if ($('#inputHint').is(':visible')) {
				$('#inputHint').fadeOut();
			}
		}
	});

	$('.input-overlay .add-on').on('click', function() {
		$(this).next('input').focus();
	});

	
	if (!$('#signin-help').is(':visible')) {
		$('.help-button').addClass('active');
		$('#signin-help').show();
	}
	

	$('.sign-in-button').removeAttr('disabled');
	$('#loginform').on('submit', function() {
    $('.sign-in-button').closest('.actions').find('.links').hide();
		$('.sign-in-button').attr('disabled','disabled').html('Please wait');
	});
});
</script>

					
