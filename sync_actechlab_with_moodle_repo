#!/bin/sh
cd /data/www/html
#keep the standard Moodle branch at actechlab repository synced with the upstream repository.
#actechlab is git@actechlab.warwick.ac.uk:moodle/moodle_code.git
#upstream is git://git.moodle.org/moodle.git
git fetch upstream
for BRANCH in MOODLE_26_STABLE; do
	git push actechlab refs/remotes/upstream/$BRANCH:$BRANCH
done
